//P390PA JOB (40700),'P390',CLASS=A,MSGCLASS=A,MSGLEVEL=(1,1),
//        NOTIFY=P390
//*
//* RUN MATRIX BATCH UTILITY PROGRAM TO DEMONSTRATE
//* WAIT AND POST OPERATIONS
//*
//* THIS JOB WAITS ON EVENT NAMED AAAAAAAA
//* THEN POSTS EVENT NAMED BBBBBBBB WHICH WILL ALLOW THE
//* NEXT JOB TO CONTINUE. TO ALLOW THIS CASCADE TO START
//* YOU MUST POST AAAAAAAA OTHER WISE IT WILL WAIT FOR EVER
//*
//* TO POST AAAAAAAA YOU COULD USE THE TSO UTILITY PROGRAM
//* OPTION 3 (EVENTS)
//*
//        EXEC PGM=SSUTIL,PARM='MXPRS001'
//STEPLIB DD DSN=DIST.MATRIX.UTILITY.LOAD,DISP=SHR
//SYSPRINT DD SYSOUT=*
//SYSIN    DD *
  CONNECT TO SERVER MTRX
  WAIT AAAAAAAA
  POST BBBBBBBB
//P390PB JOB (40700),'P390',CLASS=A,MSGCLASS=A,MSGLEVEL=(1,1),
//        NOTIFY=P390
//*
//        EXEC PGM=SSUTIL,PARM='MXPRS001'
//STEPLIB DD DSN=DIST.MATRIX.UTILITY.LOAD,DISP=SHR
//SYSPRINT DD SYSOUT=*
//SYSIN    DD *
  CONNECT TO SERVER MTRX
  WAIT BBBBBBBB
  POST CCCCCCCC
/*
//P390PC JOB (40700),'P390',CLASS=A,MSGCLASS=A,MSGLEVEL=(1,1),
//        NOTIFY=P390
//*
//        EXEC PGM=SSUTIL,PARM='MXPRS001'
//STEPLIB DD DSN=DIST.MATRIX.UTILITY.LOAD,DISP=SHR
//SYSPRINT DD SYSOUT=*
//SYSIN    DD *
  CONNECT TO SERVER MTRX
  WAIT CCCCCCCC
/*
//P390PZ JOB (40700),'P390',CLASS=A,MSGCLASS=A,MSGLEVEL=(1,1),
//        NOTIFY=P390
//*
//        EXEC PGM=SSUTIL,PARM='MXPRS001'
//STEPLIB DD DSN=DIST.MATRIX.UTILITY.LOAD,DISP=SHR
//SYSPRINT DD SYSOUT=*
//SYSIN    DD *
  CONNECT TO SERVER MTRX
  WAIT CCCCCCCC
/*
